{
  "name": "@digitalc/editor-sample",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build:development": "NODE_ENV=development vite build",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .tsx src"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.@(yml|yaml)": [
      "prettier --parser yaml --write"
    ],
    "*.md": [
      "prettier --parser markdown --write"
    ],
    "*.json": [
      "prettier --parser json --write"
    ]
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@digitalc/block-columns-container": "^0.0.1",
    "@digitalc/block-divider": "^0.0.1",
    "@digitalc/block-text": "0.0.5",
    "@digitalc/email-builder": "^0.0.7",
    "@digitalc/block-container": "^0.0.7",
    "@digitalc/block-video": "^0.0.5",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/styles": "^6.1.0",
    "@sentry/react": "^8.28.0",
    "@uiw/react-codemirror": "^4.23.0",
    "@uiw/react-markdown-editor": "^6.1.2",
    "@usewaypoint/block-avatar": "^0.0.3",
    "@usewaypoint/block-button": "^0.0.3",
    "@usewaypoint/block-heading": "^0.0.3",
    "@usewaypoint/block-html": "^0.0.3",
    "@usewaypoint/block-image": "^0.0.5",
    "@usewaypoint/block-spacer": "^0.0.3",
    "@usewaypoint/document-core": "^0.0.6",
    "axios": "^1.7.7",
    "codemirror": "^5.65.17",
    "highlight.js": "^11.9.0",
    "liquidjs": "^10.16.3",
    "notistack": "^3.0.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2",
    "vite": "^5.4.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rollup-plugin-visualizer": "^5.12.0"
  }
}
